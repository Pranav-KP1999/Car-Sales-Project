{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-datawizard-carsales"
		},
		"AzureDataLakeStorage_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_LS'"
		},
		"AzureSqlDatabase_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_LS'"
		},
		"AzureDataLakeStorage_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datawizardcardatalake.dfs.core.windows.net/"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "wizardcarsalesserver.database.windows.net"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "carSales"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminwizard"
		},
		"github_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-datawizard-carsales_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_LS_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_LS_accountKey')]"
					},
					"AzureSqlDatabase_LS_password": {
						"value": "[parameters('AzureSqlDatabase_LS_password')]"
					},
					"AzureDataLakeStorage_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LS_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_userName')]"
					},
					"github_LS_properties_typeProperties_url": {
						"value": "[parameters('github_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}